<% var exerciseId = view.basic.split(/[\\\/]/).slice(-3) %>
<script type="text/javascript">
    mule.exerciseIdRaw = "<%= exerciseId %>".split(",")
    mule.exerciseId ={
        week: mule.exerciseIdRaw[0].replace("week", ""),
        day:  mule.exerciseIdRaw[1].replace("day", ""),
        exercise: mule.exerciseIdRaw[2].replace(/exercise|challange/, "")
    }
</script>

<% var exerciseType = exerciseId.slice(-1)[0]%>

<% try { %>
    <%- include("_standardDay/_block", {title: exerciseType, file: pathJoin(view.basic, "description.md") }) -%>
<% } catch (error) {} %>
