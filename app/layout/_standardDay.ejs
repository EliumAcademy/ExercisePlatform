<% var exerciseId = view.description.split(/[\\\/]/).slice(-4, -1) %>

<script type="text/javascript">
    const exerciseId = "<%= exerciseId %>".split(",")
</script>

<% var exerciseType = exerciseId.slice(-1)[0]%>
<% exerciseType = exerciseType[0].toUpperCase() + exerciseType.slice(1,-1) + " " + exerciseType.slice(-1)[0] %>

<% try { %>
    <%- include("_standardDay/_block", {title: exerciseType, file: view.description}) -%>
<% } catch (error) {} %>
<% try { %>
    <%- include("_standardDay/_block", {title: "Try your code", file: view.try}) -%>
<% } catch (error) {} %>
<% try { %>
    <%- include("_standardDay/_block", {title: "Test", file: view.test}) -%>
<% } catch (error) {} %>

